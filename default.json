{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":semanticCommits", ":semanticCommitTypeAll(chore)", ":semanticCommitScopeDisabled"],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "gitLabAutomerge": true,
  "assignees": ["YellowGarbageBag"],
  "packageRules": [
    {
      "matchUpdateTypes": ["patch", "pin"],
      "automerge": true
    }
  ]
}
