{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  
  "prConcurrentLimit": 0,
  "prHourlyLimit": 10,
  
  "assignees": [
    "YellowGarbageBag"
  ],
  
  "packageRules": [
    {
      "matchUpdateTypes": ["patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
