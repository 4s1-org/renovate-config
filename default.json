{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "assignees": [
    "YellowGarbageBag"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "@types/node"
      ],
      "groupName": "types",
      "automerge": true,
      "automergeType": "branch"
    }
  ],
  "hostRules": [
    {
      "hostType": "npm",
      "matchHost": "https://gitlab.com/api/v4/packages/npm/",
      "encrypted": {
        "token": ""
      }
    }
  ]
}